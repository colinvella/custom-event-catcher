{
  "compilerOptions": {
    "target": "ES2020",
  "module": "CommonJS",
    "lib": ["DOM", "ES2020"],
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "types": ["chrome"]
  },
  "include": ["src"]
}
